{
  "name": "node-red-contrib-panasonic-comfort-cloud",
  "version": "1.2.4",
  "description": "Node-RED implementation of panasonic-comfort-cloud-api",
  "main": "comfort-cloud.js",
  "scripts": {
    "start": "node node_modules/node-red/red.js -v -u . -s node_modules/node-red/settings.js",
    "debug": "node --nolazy --inspect-brk=9229 ./node_modules/node-red/red.js -v -u . -s ./node_modules/node-red/settings.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bisand/node-red-contrib-panasonic-comfort-cloud.git"
  },
  "directories": {
    "examples": "./examples",
    "nodes": "./src"
  },
  "keywords": [
    "node-red",
    "panasonic",
    "comfort-cloud",
    "heatpump",
    "heat-pump"
  ],
  "author": "Andr√© Biseth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bisand/node-red-contrib-panasonic-comfort-cloud/issues"
  },
  "homepage": "https://github.com/bisand/node-red-contrib-panasonic-comfort-cloud#readme",
  "node-red": {
    "nodes": {
      "pcc-config": "./src/comfort-cloud-config.js",
      "pcc-groups": "./src/comfort-cloud-groups.js",
      "pcc-device": "./src/comfort-cloud-device.js",
      "pcc-command": "./src/comfort-cloud-command.js"
    }
  },
  "dependencies": {
    "panasonic-comfort-cloud-api": "^1.0.4"
  },
  "devDependencies": {
    "node-red": "^2.1.4"
  }
}
